{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "commonjs": true,
    "browser": true
  },
  "extends": ["standard", "standard-react"],
  "plugins": ["react", "babel", "promise", "import"],
  "rules": {
    "arrow-parens": 0,
    "generator-star-spacing": 0
  }
}
