{
  "name": "todomvc",
  "version": "0.0.1",
  "description": "A Simple Boilerplate with Webpack 4, Eslint (standard), Babel, React, MostJs, flow.",
  "main": "lib",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.conf.js --progress --mode development",
    "lint": "eslint --ext .js,jsx src --fix",
    "build": "webpack --config webpack.prod.conf.js --progress --mode production"
  },
  "dependencies": {
    "@most/core": "^1.3.2",
    "@most/dom-event": "^2.0.0",
    "@most/prelude": "^1.7.2",
    "@most/scheduler": "^1.2.1",
    "flow-bin": "^0.80.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "react-hot-loader": "^4.3.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpackbar": "^2.6.1"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/flow",
      "@babel/react"
    ],
    "plugins": [
      "@babel/proposal-object-rest-spread"
    ]
  }
}
